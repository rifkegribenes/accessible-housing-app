{
  "name": "accessible-housing-app",
  "version": "1.0.0",
  "description": "Accessible Housing Hotsheet App",
  "main": "server.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text ./node_modules/.bin/mocha --exit",
    "start": "webpack -d | node server.js",
    "prod-start": "cd .. && yarn server",
    "dev-client": "cd client && yarn build:dev && yarn start",
    "dev-server": "NODE_CONFIG_ENV=development LOG_LEVEL=debug supervisor --inspect server.js",
    "prod-server": "LOG_LEVEL=debug supervisord --inspect server.js",
    "sass": "cd client && yarn dev",
    "dev": "concurrently --kill-others-on-fail \"yarn dev-server\" \"yarn dev-client\" \"yarn sass\"",
    "snyk-protect": "snyk-protect",
    "prepare": "yarn run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rifkegribenes/accessible-housing-app.github.io.git"
  },
  "author": "Sarah Schneider",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rifkegribenes/accessible-housing-app.github.io/issues"
  },
  "homepage": "https://github.com/rifkegribenes/accessible-housing-app.github.io#readme",
  "dependencies": {
    "@snyk/protect": "latest",
    "aws": "^0.0.3-2",
    "aws-sdk": "^2.814.0",
    "axios": "^0.21.3",
    "body-parser": "^1.19.0",
    "chokidar": "^3.0.2",
    "concurrently": "^6.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "fs": "^0.0.1-security",
    "helmet": "3.21.1",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "knex": "^0.95.14",
    "knex-migrate": "^1.7.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.3",
    "multer-s3": "^2.10.0",
    "node-geocoder": "^3.25.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "request": "^2.88.0",
    "serve-favicon": "^2.5.0",
    "travis": "^0.1.1",
    "util": "^0.12.1",
    "uuid": "^3.3.2",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "aws-sdk-mock": "^4.4.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-passport-strategy": "^1.0.1",
    "husky": "^2.3.0",
    "jest-localstorage-mock": "^2.4.0",
    "knex-cleaner": "^1.3.0",
    "lint-staged": "^8.1.7",
    "local-ip-url": "^1.0.2",
    "mocha": "^9.1.3",
    "moment": "^2.24.0",
    "moxios": "^0.4.0",
    "nock": "^13.2.1",
    "nyc": "^15.1.0",
    "prettier": "^1.17.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "sinon-express-mock": "^2.2.0",
    "stream": "^0.0.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --parser flow --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "14.18.1"
  },
  "snyk": true
}
